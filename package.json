{
  "name": "@tngtech/momo-scheduler",
  "version": "0.4.1",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles --forceExit",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint --ext .ts,.js,.json .",
    "format": "prettier --write \"**/*.ts\" && eslint --fix --ext .ts,.js,.json ."
  },
  "dependencies": {
    "cron-schedule": "^3.0.6",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "2.4.0",
    "mongodb": "4.7.0",
    "typed-emitter": "2.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "9.1.2",
    "@types/human-interval": "1.0.0",
    "@types/jest": "28.1.1",
    "@types/lodash": "4.14.182",
    "@types/luxon": "2.3.2",
    "@types/node": "16.11.39",
    "@types/pino": "7.0.4",
    "@types/sinonjs__fake-timers": "8.1.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.1",
    "mongodb-memory-server": "8.7.1",
    "pino": "8.1.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "ts-mockito": "2.6.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  }
}
